{
  "manifest_version": 2,
  "name": "New Tab Page",
  "version": "1.0",
  "description": "Replaces Google Chrome's new tab page with custom page",
  "permissions": [
    "storage",
     "tabs",
     "activeTab"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png"
  }
}
